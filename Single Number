N=int(input())
lst=[]
for i in range(N):
    ele=int(input())
    lst.append(ele)
count=1
if lst[0]!=lst[1]:
    print(lst[0])
    count==0
if count==1 and lst[-1]!=lst[-1]:
    print[-1]
for i in range(1,N-1):
    if lst[i]!=lst[i-1] and lst[i]!=lst[i+1]:
        print(lst[i])
        count=0
    
    
